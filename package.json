{
  "name": "website",
  "version": "1.0.0",
  "private": true,
  "description": "Trustpoint website built with Gatsby",
  "author": "Trustpoint Project",
  "homepage": "https://trustpoint.netlify.app",
  "keywords": [
    "gatsby"
  ],
  "scripts": {
    "build": "gatsby build && sed -i '' 's|https://trustpoint.campus-schwarzwald.de/|https://trustpoint.netlify.app/|g' public/sitemap-*.xml && cp public/sitemap-index.xml public/sitemap.xml",
    "develop": "gatsby develop",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "start": "npm run develop",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing\" && exit 1"
  },
  "dependencies": {
    "gatsby": "^5.13.7",
    "gatsby-plugin-manifest": "^5.13.1",
    "gatsby-plugin-react-helmet": "^6.15.0",
    "gatsby-plugin-sitemap": "^6.15.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-helmet": "^6.1.0",
    "react-social-media-embed": "^2.5.18"
  },
  "devDependencies": {
    "prettier": "^3.3.3"
  }
}
